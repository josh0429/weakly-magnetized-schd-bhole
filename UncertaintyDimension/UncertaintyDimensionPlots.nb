88478d718184"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"TEscape", "[", 
     RowBox[{"b_", ",", "en_", ",", "\[Phi]sh_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"initVelocities", ",", "escTime"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"escTime", "=", 
        RowBox[{"MaximumT", "+", "1"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"initVelocities", "=", 
        RowBox[{"getVelocities", "[", 
         RowBox[{"b", ",", "en", ",", "\[Phi]sh"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Quiet", "@", 
        RowBox[{"NDSolve", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"ddrho", "[", "b", "]"}], ",", 
            RowBox[{"dd\[Theta]", "[", "b", "]"}], ",", 
            RowBox[{
             RowBox[{"\[Rho]", "[", "0", "]"}], "\[Equal]", "3"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"\[Rho]", "'"}], "[", "0", "]"}], "\[Equal]", 
             RowBox[{
             "initVelocities", "\[LeftDoubleBracket]", "1", 
              "\[RightDoubleBracket]"}]}], ",", 
            RowBox[{
             RowBox[{"\[Theta]", "[", "0", "]"}], "\[Equal]", 
             RowBox[{"\[Pi]", "/", "2"}]}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"\[Theta]", "'"}], "[", "0", "]"}], "\[Equal]", 
             RowBox[{
             "initVelocities", "\[LeftDoubleBracket]", "2", 
              "\[RightDoubleBracket]"}]}], ",", 
            RowBox[{"WhenEvent", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Abs", "[", 
                RowBox[{
                 RowBox[{"\[Rho]", "[", "\[Sigma]", "]"}], 
                 RowBox[{"Cos", "[", 
                  RowBox[{"\[Theta]", "[", "\[Sigma]", "]"}], "]"}]}], "]"}], 
               ">", "1000"}], ",", 
              RowBox[{
               RowBox[{"escTime", "=", "\[Sigma]"}], ";", 
               "\"\<StopIntegration\>\""}]}], "]"}], ",", 
            RowBox[{"WhenEvent", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"\[Rho]", "[", "\[Sigma]", "]"}], "\[LessEqual]", 
               "1"}], ",", 
              RowBox[{
               RowBox[{"escTime", "=", "\[Sigma]"}], ";", 
               "\"\<StopIntegration\>\""}]}], "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Rho]", ",", "\[Theta]"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Sigma]", ",", "0", ",", 
            RowBox[{"MaximumT", "+", "1"}]}], "}"}], ",", 
          RowBox[{"MaxSteps", "\[Rule]", "\[Infinity]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "escTime", "]"}]}]}], "\[IndentingNewLine]", 
     "]"}]}], ";"}], " "}]], "Input",
 CellChangeTimes->{{3.801276712334425*^9, 3.8012767127084255`*^9}, {
  3.8018031704741545`*^9, 
  3.8018031720224686`*^9}},ExpressionUUID->"6255fc18-307b-4403-a270-\
8367b71eeeae"],

Cell[TextData[{
