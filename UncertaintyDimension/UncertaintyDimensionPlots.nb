Cell[TextData[{
 "To obtain the initial conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Rho]", "'"}], "[", "0", "]"}], ",", 
     RowBox[{
      RowBox[{"\[Theta]", "'"}], "[", "0", "]"}]}], ")"}], TraditionalForm]],
  ExpressionUUID->"edae764f-a97b-4fdf-bf69-91b2e65bdf68"],
