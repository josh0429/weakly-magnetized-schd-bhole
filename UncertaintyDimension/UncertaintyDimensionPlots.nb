397792afed91"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"getUncDimension", "[", 
    RowBox[{"b_", ",", "en_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"data", ",", "numUnc", ",", "\[Phi]", ",", "slope"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"data", "=", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", 
         RowBox[{"Length", "[", "epsRange", "]"}]}], "]"}]}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{"Container", " ", "of", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Log", 
           RowBox[{"(", "\[Epsilon]", ")"}]}], ",", " ", 
          RowBox[{"Log", 
           RowBox[{"(", 
            RowBox[{"f", 
             RowBox[{"(", "\[Epsilon]", ")"}]}], ")"}]}]}], ")"}], " ", 
        "data", " ", "points"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"numUnc", "=", "0"}], ";", " ", 
         RowBox[{"(*", 
          RowBox[{
          "Number", " ", "of", " ", "uncertain", " ", "initial", " ", 
           "conditions"}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"SetSharedVariable", "[", "numUnc", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{
           "I", " ", "want", " ", "to", " ", "turn", " ", "this", " ", "into",
             " ", "a", " ", "\"\<ParallelWhile\>\"", " ", "Loop"}], ",", " ", 
           RowBox[{
            RowBox[{
             RowBox[{"i", ".", "e", ".", " ", "I"}], " ", "want", " ", "it", 
             " ", "to", " ", "loop", " ", "until", " ", "numUnc"}], "=", 
            RowBox[{"1000.", " ", "I", " ", 
             RowBox[{"don", "'"}], "t", " ", "know", " ", "how", " ", "to", 
             " ", 
             RowBox[{"yet", "."}]}]}]}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"ParallelDo", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"\[Phi]", "=", 
             RowBox[{"RandomReal", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"1", ",", "2"}], "}"}], ",", 
               RowBox[{"WorkingPrecision", "\[Rule]", "10"}]}], "]"}]}], ";", 
            " ", 
            RowBox[{"(*", 
             RowBox[{
             "Initial", " ", "conditions", " ", Cell[
              "\[Phi]\[Element][1,2]",ExpressionUUID->
               "168954a6-e768-421c-8207-666524e61157"]}], "*)"}], 
            "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"IsUncertain", "[", 
               RowBox[{"b", ",", "en", ",", "\[Phi]", ",", "\[Epsilon]"}], 
               "]"}], ",", 
              RowBox[{"numUnc", "++"}]}], "]"}], ";"}], "\[IndentingNewLine]",
            ",", "\[IndentingNewLine]", 
           RowBox[{"{", "numTraj", "}"}], "\[IndentingNewLine]", ",", 
           "\[IndentingNewLine]", 
           RowBox[{"Method", "\[Rule]", " ", "\"\<FinestGrained\>\""}]}], 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"data", "\[LeftDoubleBracket]", 
           RowBox[{"First", "@", 
            RowBox[{"Flatten", "@", 
             RowBox[{"Position", "[", 
              RowBox[{"epsRange", ",", "\[Epsilon]"}], "]"}]}]}], 
           "\[RightDoubleBracket]"}], "=", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Log", "[", "\[Epsilon]", "]"}], ",", 
            RowBox[{"Log", "[", 
             RowBox[{"numUnc", "/", "numTraj"}], "]"}]}], "}"}]}], ";"}], 
        "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"\[Epsilon]", ",", "epsRange"}], "}"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"slope", "=", 
       RowBox[{"bestFitSlope", "[", "data", "]"}]}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{"Get", " ", "the", " ", "slope"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"1", "-", "slope"}], "]"}]}]}], "\[IndentingNewLine]", "]"}]}],
   ";"}]], "Input",
