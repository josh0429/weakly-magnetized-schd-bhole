b0398a6e1e1d"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"getVelocities", "[", 
    RowBox[{"b_", ",", "en_", ",", "\[Phi]sh_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"d\[Rho]0", ",", "d\[Theta]0", ",", "result"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"d\[Rho]0", "=", 
       RowBox[{
        RowBox[{"(", 
         SqrtBox[
          FractionBox[
           RowBox[{
            SuperscriptBox["en", "2"], "-", 
            RowBox[{"Ueff", "[", "b", "]"}]}], 
           RowBox[{
            SuperscriptBox[
             RowBox[{"Sin", "[", "\[Phi]sh", "]"}], "2"], "+", 
            RowBox[{"6", 
             SuperscriptBox[
              RowBox[{"Cos", "[", "\[Phi]sh", "]"}], "2"]}]}]]], ")"}], 
        RowBox[{"Sin", "[", "\[Phi]sh", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"d\[Theta]0", "=", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"(", 
          SqrtBox[
           FractionBox[
            RowBox[{
             SuperscriptBox["en", "2"], "-", 
             RowBox[{"Ueff", "[", "b", "]"}]}], 
            RowBox[{
             SuperscriptBox[
              RowBox[{"Sin", "[", "\[Phi]sh", "]"}], "2"], "+", 
             RowBox[{"6", 
              SuperscriptBox[
               RowBox[{"Cos", "[", "\[Phi]sh", "]"}], "2"]}]}]]], ")"}]}], 
        RowBox[{"Cos", "[", "\[Phi]sh", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"result", "=", 
       RowBox[{"{", 
        RowBox[{"d\[Rho]0", ",", "d\[Theta]0"}], "}"}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
