c872a5316db7"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"IsUncertain", "[", 
    RowBox[{"b_", ",", "en_", ",", "\[Phi]sh_", ",", "epsilon_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"result", ",", "T0", ",", "T1"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"T0", "=", 
       RowBox[{"TEscape", "[", 
        RowBox[{"b", ",", "en", ",", "\[Phi]sh"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"T1", "=", 
       RowBox[{"TEscape", "[", 
        RowBox[{"b", ",", "en", ",", 
         RowBox[{"\[Phi]sh", "+", "epsilon"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Abs", "[", 
          RowBox[{"T0", "-", "T1"}], "]"}], ">", "0.005"}], ",", 
        RowBox[{"result", "=", "True"}], ",", 
        RowBox[{"result", "=", "False"}]}], "]"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.801277123808025*^9, 3.801277159150511*^9}, {
